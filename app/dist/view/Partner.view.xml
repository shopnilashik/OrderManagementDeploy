<mvc:View
	controllerName="derga.ordermanagement.controller.Partner"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:t="sap.ui.table"
	xmlns="sap.m"
>
	<FlexBox
		id="PartnerMainFlexBox"
		width="100%"
	>
		<FlexBox
			id="LeftFlexBoxTable"
			width="50%"
		>
			<t:Table
				rows="{oOrderData>/Partner}"
				visibleRowCount="6"
				selectionMode="None"
			>
				<t:columns>
					<t:Column>
						<Label text="{i18n>item.page.patnerName}"/>
						<t:template>
							<Text
								text="{oOrderData>patnerName}"
								wrapping="false"
							/>
						</t:template>
					</t:Column>
					<t:Column>
						<Label text="{i18n>item.page.partnerRole}"/>
						<t:template>
							<Text
								text="{oOrderData>partnerRole}"
								wrapping="false"
							/>
						</t:template>
					</t:Column>
					<t:Column>
						<Label text="{i18n>item.page.partner}"/>
						<t:template>
							<Text
								text="{oOrderData>partner}"
								wrapping="false"
							/>
						</t:template>
					</t:Column>
					<t:Column hAlign="End">
						<Label
							text="{i18n>item.page.edit}"
							class="sapUiTinyMarginEnd"
						/>
						<t:template>
							<Button
								id="save_button_edit"
								icon="sap-icon://value-help"
								type="Transparent"
							/>
						</t:template>
					</t:Column>
				</t:columns>
			</t:Table>
		</FlexBox>
		<FlexBox
			id="RightContentFlexBox"
			class="sapUiTinyMargin"
			direction="Column"
			width="50%"
		>
			<Label
				id="recipientLabel"
				text="{i18n>item.page.recipient}"
				class="sapUiSmallMarginBegin"
				design="Bold"
			/>
			<FlexBox
				id="RightContantRow_1"
				class="sapUiSmallMarginBegin sapUiTinyMarginBottom"
				justifyContent="SpaceBetween"
				width="100%"
			>
				<FlexBox
					id="consigneegoodsFlexBox"
					class="sapUiSmallMarginEnd"
					direction="Column"
					width="50%"
				>
					<items>
						<Label
							id="consigneegoodsLabel"
							text="{i18n>item.page.consigneegoods}"
						/>
						<Input
							id="consigneegoodsInput"
							showValueHelp="true"
							valueHelpOnly="true"
							width="100%"
							placeholder="Select Shipping Code"
							class=""
						/>
					</items>
				</FlexBox>
				<FlexBox
					id="nameConsigneeGoodsFlexBox"
					class=""
					direction="Column"
					width="50%"
				>
					<items>
						<Label
							id="nameConsigneeGoodsLebel"
							text="{i18n>item.page.nameConsigneeGoods}"
						/>
						<Input
							id="nameConsigneeGoodsInput"
							placeholder="NAME1_WE+NAME2_WE"
						/>
					</items>
				</FlexBox>
			</FlexBox>
			<FlexBox
				id="RightContantRow_2"
				class="sapUiSmallMarginBegin sapUiTinyMarginBottom"
				direction="Column"
				width="100%"
			>
				<FlexBox
					id="viapiazzaFlexBox"
					class=""
					direction="Column"
					width="100%"
				>
					<items>
						<Label
							id="viapiazzaLebel"
							text="{i18n>item.page.viapiazza}"
						/>
						<Input
							id="viapiazzaInput"
							placeholder="NAME1_WE+NAME2_WE"
						/>
					</items>
				</FlexBox>
			</FlexBox>
			<FlexBox
				id="RightContantRow_3"
				class="sapUiSmallMarginBegin sapUiTinyMarginBottom"
				justifyContent="SpaceBetween"
				width="100%"
			>
				<FlexBox
					id="capFlexBox"
					class="sapUiSmallMarginEnd"
					direction="Column"
					width="50%"
				>
					<items>
						<Label
							id="capLebel"
							text="{i18n>item.page.cap}"
						/>
						<Input
							id="InputPSTLZ"
							placeholder="PSTLZ_WE"
						/>
					</items>
				</FlexBox>
				<FlexBox
					id="cityFlexBox"
					class=""
					direction="Column"
					width="50%"
				>
					<items>
						<Label
							id="cityLebel"
							text="{i18n>item.page.city}"
						/>
						<Input
							id="InputORT01"
							placeholder="ORT01_WE"
						/>
					</items>
				</FlexBox>
			</FlexBox>
			<FlexBox
				id="RightContantRow_4"
				class="sapUiSmallMarginBegin"
				justifyContent="SpaceBetween"
				width="100%"
			>
				<FlexBox
					id="provinceFlexBox"
					class="sapUiSmallMarginEnd"
					direction="Column"
					width="50%"
				>
					<items>
						<Label
							id="provinceLebel"
							text="{i18n>item.page.province}"
						/>
						<ComboBox
							id="province1"
							width="100%"
							placeholder="LAND1_WE"
							items="{
								path: '/Regions',
								length: '1000000'
							}"
							change="destionationRegionAfterSelect"
						>
							<items>
								<core:ListItem
									enabled="true"
									key="{ID}"
									text="{name}"
									additionalText="{ID}"
									textDirection="Inherit"
								/>
							</items>
						</ComboBox>
					</items>
				</FlexBox>
				<FlexBox
					id="countryFlexBox"
					class=""
					direction="Column"
					width="50%"
				>
					<items>
						<Label
							id="countryLebel"
							text="{i18n>item.page.country}"
						/>
						<ComboBox
							id="selectLAND1"
							width="100%"
							items="{
								path: '/Countries',
								length: '100000'
							}"
							selectionChange="selectCountryOne"
							showSecondaryValues="true"
							change="destionationCountryAfterSelect"
							placeholder="LAND1_WE"
						>
							<items>
								<core:ListItem
									key="{ID}"
									text="{name}"
									additionalText="{ID}"
								/>
							</items>
						</ComboBox>
					</items>
				</FlexBox>
			</FlexBox>
			<Label
				id="ForwarderLabel"
				text="{i18n>forwarder}"
				class="sapUiSmallMarginBegin sapUiSmallMarginTop"
				design="Bold"
			/>
			<FlexBox
				id="RightContantRow_5"
				class="sapUiSmallMarginBegin"
				justifyContent="SpaceBetween"
				width="100%"
			>
				<FlexBox
					id="forwarderFlexBox"
					class="sapUiSmallMarginEnd"
					direction="Column"
					width="50%"
				>
					<items>
						<Label
							id="Lebelforwarder"
							text="{i18n>item.page.forwarder}"
						/>
						<Input
							id="forwarderInput"
							showValueHelp="true"
							valueHelpOnly="true"
							width="100%"
							class="sapUiSmallMarginBottom"
							placeholder="Select Shipping Code"
						/>
					</items>
				</FlexBox>
				<FlexBox
					id="recipientNameFlexBox"
					class=""
					direction="Column"
					width="50%"
				>
					<items>
						<Label
							id="recipientNameLebel"
							text="{i18n>item.page.recipientName}"
						/>
						<Input
							id="InputNAME1"
							placeholder="NAME1_WE+NAME2_WE"
						/>
					</items>
				</FlexBox>
			</FlexBox>
			<FlexBox
				id="RightContantRow_6"
				class="sapUiSmallMarginBegin sapUiTinyMarginBottom"
				direction="Column"
				width="100%"
			>
				<FlexBox
					id="viapiazzaFlexBox2"
					class=""
					direction="Column"
					width="100%"
				>
					<items>
						<Label
							id="viapiazzaLebel2"
							text="{i18n>item.page.viapiazza}"
						/>
						<Input
							id="InputStars_sp"
							placeholder="NAME1_WE+NAME2_WE"
						/>
					</items>
				</FlexBox>
			</FlexBox>
			<FlexBox
				id="RightContantRow_7"
				class="sapUiSmallMarginBegin sapUiTinyMarginBottom"
				justifyContent="SpaceBetween"
				width="100%"
			>
				<FlexBox
					id="capFlexBox2"
					class="sapUiSmallMarginEnd"
					direction="Column"
					width="50%"
				>
					<items>
						<Label
							id="capLebel2"
							text="{i18n>item.page.cap}"
						/>
						<Input
							id="InputPSTLZ_SP"
							placeholder="PSTLZ_WE"
						/>
					</items>
				</FlexBox>
				<FlexBox
					id="cityFlexBox2"
					class=""
					direction="Column"
					width="50%"
				>
					<items>
						<Label
							id="cityLebel2"
							text="{i18n>item.page.city}"
						/>
						<Input
							id="InputORT01_SP"
							placeholder="ORT01_WE"
						/>
					</items>
				</FlexBox>
			</FlexBox>
			<FlexBox
				id="RightContantRow_8"
				class="sapUiSmallMarginBegin sapUiTinyMarginBottom"
				justifyContent="SpaceBetween"
				width="100%"
			>
				<FlexBox
					id="provinceFlexBox2"
					class="sapUiSmallMarginEnd"
					direction="Column"
					width="50%"
				>
					<items>
						<Label
							id="provinceLebel2"
							text="{i18n>item.page.province}"
						/>
						<ComboBox
							id="province2"
							width="100%"
							items="{
								path: '/Regions',
								length: '1000000'
							}"
							placeholder="REGIO_WE"
							change="shipmentRegionAfterSelect"
							showSecondaryValues="true"
							
						>
							<items>
								<core:ListItem
									enabled="true"
									key="{ID}"
									text="{name}"
									additionalText="{ID}"
									textDirection="Inherit"
								/>
							</items>
						</ComboBox>
					</items>
				</FlexBox>
				<FlexBox
					id="countryFlexBox2"
					class=""
					direction="Column"
					width="50%"
				>
					<items>
						<Label
							id="countryLebel2"
							text="{i18n>item.page.country}"
						/>
						<ComboBox
							id="selectLAND1_2"
							width="100%"
							items="{
								path: '/Countries',
								length: '100000'
							}"
							selectionChange="selectCountryTwo"
							showSecondaryValues="true"
							change="destionationCountryAfterSelect"
							placeholder="LAND1_WE"
						>
							<items>
								<core:ListItem
									key="{ID}"
									text="{name}"
									additionalText="{ID}"
								/>
							</items>
						</ComboBox>
					</items>
				</FlexBox>
			</FlexBox>
		</FlexBox>
	</FlexBox>
</mvc:View>